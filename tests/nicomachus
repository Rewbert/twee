a/0 = 1
0/0 = 1
1/0 = 1
s/1 = 1
+/2 = 1
*/2 = 1
sum/1 = 1
cubes/1 = 1
--
+(X, Y) = +(Y, X)
+(X, +(Y, Z)) = +(+(X, Y), Z)
*(X, Y) = *(Y, X)
*(X, *(Y, Z)) = *(*(X, Y), Z)
+(X, 0) = X
*(X, 0) = 0
*(X, 1) = X
*(X, +(Y, Z)) = +(*(X, Y), *(X, Z))
*(+(X, Y), Z) = +(*(X, Z), *(Y, Z))
+(s(X), Y) = s(+(X, Y))
*(s(X), Y) = +(Y, *(X, Y))
sum(0) = 0
sum(s(N)) = +(s(N), sum(N))
cubes(0) = 0
%cubes(s(N)) = +(*(s(N), *(s(N), s(N))), cubes(N))
%+(sum(N), sum(N)) = *(N, s(N))
%*(sum(a), sum(a)) = cubes(a)
--
*(sum(s(a)), sum(s(a)))
cubes(s(a))
