cnf(a, axiom, '+'(X, Y) = '+'(Y, X)).
cnf(a, axiom, '+'(X, '+'(Y, Z)) = '+'('+'(X, Y), Z)).
cnf(a, axiom, '*'(X, Y) = '*'(Y, X)).
cnf(a, axiom, '*'(X, '*'(Y, Z)) = '*'('*'(X, Y), Z)).
cnf(a, axiom, '+'(X, '0') = X).
cnf(a, axiom, '*'(X, '0') = '0').
cnf(a, axiom, '*'(X, '1') = X).
cnf(a, axiom, '*'(X, '+'(Y, Z)) = '+'('*'(X, Y), '*'(X, Z))).
cnf(a, axiom, '*'('+'(X, Y), Z) = '+'('*'(X, Z), '*'(Y, Z))).
cnf(a, axiom, '+'(s(X), Y) = s('+'(X, Y))).
cnf(a, axiom, '*'(s(X), Y) = '+'(Y, '*'(X, Y))).
cnf(a, axiom, sum('0') = '0').
cnf(a, axiom, sum(s(N)) = '+'(s(N), sum(N))).
cnf(a, axiom, cubes('0') = '0').
%cubes(s(N)) = '+'('*'(s(N), '*'(s(N), s(N))), cubes(N))
%'+'(sum(N), sum(N)) = '*'(N, s(N))
%'*'(sum(a), sum(a)) = cubes(a)
cnf(a, axiom, '*'(sum(s(a)), sum(s(a))) != cubes(s(a))).
