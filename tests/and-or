0/0 = 1
1/0 = 1
+/2 = 1
*/2 = 1
not/1 = 1
--
+(X, Y) = +(Y, X)
+(X, +(Y, Z)) = +(+(X, Y), Z)
*(X, Y) = *(Y, X)
*(X, *(Y, Z)) = *(*(X, Y), Z)
+(X, 0) = X
*(X, 0) = 0
*(X, +(Y, Z)) = +(*(X, Y), *(X, Z))
+(X, *(Y, Z)) = *(+(X, Y), +(X, Z))
not(not(X)) = X
not(+(X, Y)) = *(not(X), not(Y))
+(X, not(X)) = 1
*(X, not(X)) = 0
--
