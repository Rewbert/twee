cnf(a, axiom, '->'(true, X) = X).
cnf(a, axiom, '->'('->'(X, Y), '->'('->'(Y, Z), '->'(X, Z))) = true).
cnf(a, axiom, '->'('->'(not(X), not(Y)), '->'(Y, X)) = true).
cnf(a, axiom, '->'('->'(X, Y), Y) = '->'('->'(Y, X), X)).
cnf(a, axiom, or(X, Y) = '->'(not(X), Y)).
cnf(a, axiom, or(a,or(b,c)) != or(or(a,b),c)).
